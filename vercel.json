{
    "headers": [
        {
          "source": "/(.*).(js|css)",
          "headers": [
            {
              "key": "Content-Type",
              "value": "text/$2"
            }
          ]
        }
      ],

    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  